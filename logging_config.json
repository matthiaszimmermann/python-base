{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s %(relpath)s:%(lineno)d %(levelname)s %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "standard"
        }
    },
    "loggers": {
        "": {
            "level": "DEBUG",
            "handlers": [
                "console"
            ],
            "propagate": true
        }
    }
}
